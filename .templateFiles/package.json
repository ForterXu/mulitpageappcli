{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "index.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./conf/webpack.dev.js",
    "dev-build": "webpack --config ./conf/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./conf/webpack.prod.js",
    "build:dll": "webpack --config ./conf/webpack.dll.js"
  },
  "keywords": [
    "{{ name }}",
    "app"
  ],
  "author": "{{ author }}",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
